{
  "manifest_version": 3,
  "name": "Prisma - Classy Minecraft Wallpapers",
  "version": "1.0",
  "description": "A Fancy Minecraft Theme to light up your new tabs!! New dynamic background/wallpaper on each refresh.",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "https://api.github.com/*",
    "https://raw.githubusercontent.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  }
}