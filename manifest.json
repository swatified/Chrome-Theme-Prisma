{
  "manifest_version": 3,
  "name": "Prisma",
  "version": "1.0",
  "description": "A Fancy Minecraft Theme to light up your new tabs!! New background images on each refresh.",
  "permissions": [
    "storage",
    "tabs"
  ],
  "host_permissions": [
    "https://api.github.com/*",
    "https://raw.githubusercontent.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  }
}